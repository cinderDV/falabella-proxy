{
    "name": "empresa/falabella-proxy",
    "description": "Wrapper para Falabella Seller API usando Rocket Labs SDK",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.6",
        "vlucas/phpdotenv": "^5.5",
        "rocket-labs/sellercenter-sdk-php": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GFG/seller-center-sdk-php"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "cweagans/composer-patches": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "patches": {
            "rocket-labs/sellercenter-sdk-php": [
                "patches/rocket-labs-php83-filter-constants.patch"
            ]
        }
    }
}
